{
    "search.exclude": {
        "**/*.html": true,
        "**/*.css": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "files.exclude": {
        "**/*.html": true,
        "**/*.css": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "luaide-lite-up.enableDiagnostic": false,
    "luaide-lite-up.debugLanguageServer": false,
    "[lua]": {
        "editor.defaultFormatter": "mayishidai.luaide-lite-up"
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.automationShell.windows": "cmd",
    "terminal.integrated.gpuAcceleration": "off",
    "Lua.runtime.version": "Lua 5.1",
    "Lua.diagnostics.enable": false,
    "Lua.diagnostics.globals": [
        "utils",
        "stringx",
        "tablex",
        "pretty"
    ],
    "Lua.telemetry.enable": false
}